<%= form_for @note do |f| %>
	<%= f.label :title %><br/>
	<%= f.text_field :title %><br/>

	<%= f.label :content %><br/>
	<%= f.text_area :content, :rows => 6, :cols => 45 %><br/>

	<%= f.label :keywords %><br/>
	<%= f.text_field :keywords %><br/>
	
	<%= f.submit "Create note", :class => "submit"%>
<% end %>
	

